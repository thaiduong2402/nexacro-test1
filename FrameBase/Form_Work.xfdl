	"strCode",
	"ScvUrl::select_code.jsp?arg=1&arg2=2",
	"",
	"ds_deparmant = out_dept",
	"arg=1 arg=" + nexacro.wrapQuote("2 3"),
	"fn_call"
	);	
};
]]></Script>
